@using ControleDeMedicamentos.ConsoleApp.ModuloFuncionarios
@{
    List<Funcionario> funcionarios  = ViewBag.Funcionarios;
}
    <!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title> Controle de Medicamentos </title> 
</head>
<body>
    <hr />
    <h3> <a href="/">  PaginaInicial </a> </h3>
    <h1> Controle de Medicamentos </h1>
    <hr />

    <h3> Funcionarios - Visualização</h3>
    
    
    <ul>
        @foreach(Funcionario f in funcionarios)
        {
           <li> 
           @f.ToString() | 
           <a href="/funcionario/editar/@f.Id"> Editar </a> |
           <a href="/funcionario/excluir/@f.Id"> Excluir </a> 
           </li>
        }
    </ul>
</body>
</html>